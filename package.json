  "description": "For easy styling of react native components and its style composition",
  "main": "lib/index.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "rm -rf ./lib && ./node_modules/.bin/babel -e 0 ./src --out-dir ./lib"
  },
  "peerDependencies": {
    "react": "*",
